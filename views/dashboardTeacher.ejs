									<% user.subjectsTaught.forEach((subject, index)=>{ %>
									<li class="d-flex flex-row">
										<a href="#">
											<%= index+1 %>
											<form
												action="/teacher/<%= user._id %>/delete/subjects"
												method="post"
											>
												<input
													type="hidden"
													name="subjectToDelete"
													value="<%=subject %>"
												/>
												<button
													type="submit"
													class="btn btn-sm uk-button-danger"
												>
													<span data-uk-icon="icon: trash"></span>
												</button>
											</form>

											<span data-uk-icon="icon: info"></span>
											<%= subject %>
										</a>
									</li>

