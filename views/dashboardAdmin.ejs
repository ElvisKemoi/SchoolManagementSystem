									<option selected disabled>Select Class Teacher</option>
									<% allTeachers.forEach((teacher)=>{ %> 
										<option value="<%= teacher.username%>"  > <%= teacher.username %> </option>
									<% }) %>
								
								  </select>
							</div>
						
							
							
							<div class="mb-3"><button type="submit" class="btn btn-primary w-100 mx-auto">Create Class</button></div>
						</div>
					</form>
